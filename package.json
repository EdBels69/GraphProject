{
  "name": "graph-analyser",
  "version": "1.0.0",
  "description": "Scientific Article Analysis and Graph Visualization Application",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "tsx watch api/server.ts",
    "build": "npm run build:ui && npm run build:api",
    "build:ui": "vite build",
    "build:api": "tsc -p tsconfig.server.json && cp -R api/core/prisma dist/api/core/ && echo '{\"type\": \"commonjs\"}' > dist/package.json",
    "start": "node dist/api/server.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e": "tsx tests/e2e/critical_path.test.ts",
    "test:coverage": "vitest --coverage",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "cli": "tsx cli/index.ts"
  },
  "dependencies": {
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@fontsource/outfit": "^5.2.8",
    "@fontsource/rajdhani": "^5.2.7",
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.45.0",
    "@types/archiver": "^7.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "archiver": "^7.0.1",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "docx": "^9.5.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.24.10",
    "i18next": "^25.7.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.562.0",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "parquetjs-lite": "^0.8.7",
    "pdf-parse": "^1.1.1",
    "piscina": "^5.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-force-graph-2d": "^1.29.0",
    "react-i18next": "^16.5.2",
    "react-router-dom": "^6.22.3",
    "recharts": "^3.6.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^3.4.0",
    "uuid": "^9.0.1",
    "zod": "^4.3.5",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/file-saver": "^2.0.7",
    "@types/multer": "^2.0.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.8.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.38",
    "prisma": "^5.22.0",
    "supertest": "^7.2.2",
    "tailwindcss": "^3.4.12",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  }
}
